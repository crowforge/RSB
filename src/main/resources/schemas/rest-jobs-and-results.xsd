<xsd:schema
    attributeFormDefault="unqualified"
    elementFormDefault="unqualified"
    version="1.0"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    jaxb:version="2.0"
    xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    jaxb:extensionBindingPrefixes="xjc">

    <xsd:annotation>
        <xsd:appinfo>
            <jaxb:globalBindings>
                <xjc:simple />
            </jaxb:globalBindings>
            <jaxb:schemaBindings>
                <jaxb:package
                    name="eu.openanalytics.rsb.rest.types" />
            </jaxb:schemaBindings>
        </xsd:appinfo>
    </xsd:annotation>

    <xsd:element
        name="r-job-accepted"
        type="RJobAcceptedType" />
    <xsd:complexType
        name="RJobAcceptedType">
        <xsd:attribute
            name="jobId"
            type="xsd:string"
            use="required" />
        <xsd:attribute
            name="appName"
            type="xsd:string"
            use="required" />
        <xsd:attribute
            name="resultUri"
            type="xsd:anyURI"
            use="required" />
    </xsd:complexType>
</xsd:schema>